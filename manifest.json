{
  "manifest_version": 2,
  "name": "Twitter Scraper Browser Add-on",
  "version": "0.1.0",
  "description": "Export twitter data",
  "browser_action": {
    "default_title": "Twitter Scraper",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": ["background-script.js"]
  },
  "permissions": ["https://twitter.com/*", "storage", "tabs"]
}
